{
  "name": "zui",
  "version": "0.0.1",
  "description": "Mobile ui library created zbfe ",
  "scripts": {
    "check": "fecs check src/ test/ --reporter=baidu",
    "test": "karma start karma.conf.js",
    "test-cov": "npm test -- --reporters progress,coverage",
    "test-watch": "npm test -- --auto-watch --no-single-run",
    "hook-install": "prepush install",
    "hook-remove": "prepush remove",
    "start": "npm run server",
    "server": "http-server",
    "build:gh-pages": "node build-gh-pages.js"
  },
  "prepush": [
    "npm run check"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zbfe/zui.git"
  },
  "author": "zbfe",
  "license": "MIT",
  "devDependencies": {
    "fecs": ">=0.6.3",
    "http-server": ">=0.9.0",
    "jasmine-core": "2.4.1",
    "karma": ">=0.13.22",
    "karma-coverage": ">=0.5.5",
    "karma-jasmine": ">=0.3.7",
    "karma-phantomjs-launcher": ">=1.0.0",
    "karma-requirejs": "^1.0.0",
    "prepush": ">=3.1.8",
    "requirejs": "2.2.0"
  }
}
